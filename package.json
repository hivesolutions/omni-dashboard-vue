    "files": [
        "index.html",
        "src/**/*.css",
        "src/**/*.js",
        "src/**/*.svg",
        "src/**/*.png",
        "src/**/*.html"
    ],
    "main": "app.js",
    "scripts": {
        "build": "webpack --mode production",
        "build-dev": "webpack --mode development",
        "dev": "webpack-dev-server --mode development --watch --hot",
        "lint": "eslint \"./src/**/*.{js,vue}\"",
        "prettier": "prettier \"./*.{js,json}\" \"./!(dist)/**/*.{js,json}\" --write",
        "production": "webpack-dev-server --mode production",
        "start": "http-server ./dist",
        "upgrade": "npx sort-package-json && ncu -au",
        "watch": "webpack --mode development --watch",
        "webpack": "webpack"
    },
    "dependencies": {
        "hive-js-util": "^0.2.0",
        "http-server": "^0.11.1",
        "loaders.css": "^0.1.2",
        "vue": "^2.6.10",
        "vue-carousel": "0.17.0",
        "vue-global-events": "^1.1.2",
        "vue-resource": "^1.5.1"
    },
    "devDependencies": {
