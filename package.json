{
    "name": "omni-dashboard-vue",
    "version": "0.1.0",
    "description": "Simple Web App for Dashboarding",
    "keywords": [
        "dashboard",
        "omni",
        "vue"
    ],
    "homepage": "https://github.com/hivesolutions/omni_dashboard_vue#readme",
    "bugs": {
        "url": "https://github.com/hivesolutions/omni_dashboard_vue/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hivesolutions/omni_dashboard_vue.git"
    },
    "license": "Apache-2.0",
    "author": "Hive Solutions",
    "files": [
        "index.html",
        "src/**/*.css",
        "src/**/*.js",
        "src/**/*.svg",
        "src/**/*.png",
        "src/**/*.html"
    ],
    "main": "app.js",
    "scripts": {
        "build": "webpack --mode production",
        "build-dev": "webpack --mode development",
        "dev": "webpack-dev-server --mode development --watch --hot",
        "lint": "eslint \"./src/**/*.{js,vue}\"",
        "lint-all": "npm run lint && npm run stylelint",
        "lint-fix": "eslint \"./src/**/*.{js,vue}\" --fix",
        "prettier": "prettier \"./*.{js,json,vue}\" \"./!(dist)/**/*.{js,json,vue}\" --write",
        "pretty": "npm run prettier && npm run lint-fix && npm run stylelint-fix",
        "production": "webpack-dev-server --mode production",
        "start": "http-server ./dist",
        "stylelint": "stylelint \"./**/*.{css,scss,vue}\"",
        "stylelint-fix": "stylelint \"./**/*.{css,scss,vue}\" --fix",
        "upgrade": "npx sort-package-json && ncu -u",
        "watch": "webpack --mode development --watch",
        "webpack": "webpack"
    },
    "dependencies": {
        "hive-js-util": "^0.2.2",
        "http-server": "^0.12.0",
        "loaders.css": "^0.1.2",
        "vue": "^2.6.11",
        "vue-carousel": "^0.18.0",
        "vue-global-events": "^1.1.2",
        "vue-resource": "^1.5.1"
    },
    "devDependencies": {
        "@babel/core": "^7.7.5",
        "@babel/preset-env": "^7.7.6",
        "babel-loader": "^8.0.6",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "css-loader": "^3.3.2",
        "eslint": "^6.7.2",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-config-hive": "^0.2.2",
        "eslint-config-prettier": "^6.7.0",
        "eslint-config-ssense": "^0.2.0",
        "eslint-loader": "^3.0.3",
        "eslint-plugin-flowtype": "^4.5.2",
        "eslint-plugin-fp": "^2.3.0",
        "eslint-plugin-import": "^2.19.1",
        "eslint-plugin-prettier": "^3.1.2",
        "extract-loader": "^3.1.0",
        "file-loader": "^5.0.2",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "npm-check-updates": "^4.0.1",
        "offline-plugin": "^5.0.7",
        "prettier": "^1.19.1",
        "prettier-config-hive": "^0.1.1",
        "sass-loader": "^8.0.0",
        "sort-package-json": "^1.31.0",
        "style-loader": "^1.0.1",
        "stylelint": "^12.0.0",
        "stylelint-config-hive": "^0.1.7",
        "typeface-open-sans": "^0.0.75",
        "vue-loader": "^15.7.2",
        "vue-template-compiler": "^2.6.11",
        "webpack": "^4.41.2",
        "webpack-bundle-analyzer": "^3.6.0",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.9.0",
        "webpack-manifest-plugin": "^2.2.0",
        "webpack-pwa-manifest": "^4.1.1"
    }
}
