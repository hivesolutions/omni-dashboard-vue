{
    "name": "omni-dashboard-vue",
    "version": "0.1.0",
    "description": "Simple Web App for Dashboarding",
    "keywords": [
        "dashboard",
        "omni",
        "vue"
    ],
    "homepage": "https://github.com/hivesolutions/omni_dashboard_vue#readme",
    "bugs": {
        "url": "https://github.com/hivesolutions/omni_dashboard_vue/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hivesolutions/omni_dashboard_vue.git"
    },
    "license": "Apache-2.0",
    "author": "Hive Solutions",
    "main": "app.js",
    "files": [
        "index.html",
        "src/**/*.css",
        "src/**/*.js",
        "src/**/*.svg",
        "src/**/*.png",
        "src/**/*.html"
    ],
    "scripts": {
        "build": "cross-env NODE_ENV=production webpack --mode production",
        "build-dev": "cross-env NODE_ENV=development webpack --mode development",
        "dev": "cross-env NODE_ENV=development webpack serve --mode development --hot",
        "lint": "eslint \"./src/**/*.{js,vue}\"",
        "lint-all": "npm run lint && npm run stylelint",
        "lint-fix": "eslint \"./src/**/*.{js,vue}\" --fix",
        "prettier": "prettier \"./*.{js,json,vue}\" \"./!(dist)/**/*.{js,json,vue}\" --write",
        "pretty": "npm run prettier && npm run lint-fix && npm run stylelint-fix && npm run svgo",
        "production": "cross-env NODE_ENV=production webpack-dev-server --mode production",
        "start": "http-server ./dist",
        "stylelint": "stylelint \"./**/*.{css,scss,vue}\"",
        "stylelint-fix": "stylelint \"./**/*.{css,scss,vue}\" --fix",
        "svgo": "svgo -r .",
        "upgrade": "npx sort-package-json && ncu -u",
        "watch": "cross-env NODE_ENV=development webpack --mode development --watch",
        "webpack": "webpack"
    },
    "dependencies": {
        "hive-js-util": "^0.4.1",
        "http-server": "^14.1.1",
        "loaders.css": "^0.1.2",
        "vue": "^2.7.13",
        "vue-carousel": "^0.18.0",
        "vue-global-events": "^1.2.1",
        "vue-resource": "^1.5.3"
    },
    "devDependencies": {
        "@babel/core": "^7.20.2",
        "@babel/preset-env": "^7.20.2",
        "babel-loader": "^9.1.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.1",
        "eslint": "^8.27.0",
        "eslint-config-hive": "^0.6.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-mocha": "^10.1.0",
        "eslint-plugin-n": "^15.5.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-vue": "^9.7.0",
        "eslint-webpack-plugin": "^3.2.0",
        "extract-loader": "^5.1.0",
        "file-loader": "^6.2.0",
        "html-loader": "^4.2.0",
        "html-webpack-plugin": "^5.5.0",
        "npm-check-updates": "^16.3.16",
        "postcss": "^8.4.18",
        "postcss-html": "^1.5.0",
        "prettier": "^2.7.1",
        "prettier-config-hive": "^0.1.7",
        "sass-loader": "^13.1.0",
        "sort-package-json": "^2.1.0",
        "style-loader": "^3.3.1",
        "stylelint": "^14.14.1",
        "stylelint-config-hive": "^1.1.0",
        "stylelint-config-html": "^1.1.0",
        "stylelint-config-standard": "^29.0.0",
        "stylelint-config-standard-scss": "^6.1.0",
        "stylelint-order": "^5.0.0",
        "svgo": "^3.0.0",
        "typeface-open-sans": "^1.1.13",
        "vue-loader": "^15.10.0",
        "vue-template-compiler": "^2.7.13",
        "webpack": "^5.74.0",
        "webpack-bundle-analyzer": "^4.7.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.11.1",
        "webpack-manifest-plugin": "^5.0.0"
    }
}
