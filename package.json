        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "css-loader": "^2.1.1",
        "eslint": "^5.15.3",
        "eslint-config-hive": "^0.1.8",
        "eslint-loader": "^2.1.2",
